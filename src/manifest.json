{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "http://localhost:8080/",
  "description": "A Vue Browser Extension",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "24": "icons/icon24.png",
    "36": "icons/icon36.png",
    "48": "icons/icon48.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__",
    "default_icon": {
      "24": "icons/icon24.png",
      "36": "icons/icon36.png",
      "48": "icons/icon48.png"
    }
  }
}
